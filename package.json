{
  "name": "voice-vibe-coding",
  "displayName": "Voice Vibe Coding",
  "description": "Seamless AI-powered voice coding with Fn key activation - hands-free coding for accessibility and productivity. Compatible with VSCode, VSCodium, Cursor, Windsurf, and other OpenVSX-supported editors.",
  "version": "0.1.0",
  "publisher": "voice-vibe-team",
  "engines": {
    "vscode": ">=1.80.0"
  },
  "categories": ["Other"],
  "keywords": ["voice", "accessibility", "ai", "hands-free", "carpal-tunnel", "speech-recognition", "openvsx", "vscodium", "cursor", "windsurf"],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "voiceVibeCoding.helloWorld",
        "title": "Hello World",
        "category": "Voice Vibe Coding"
      },
      {
        "command": "voiceVibeCoding.startVoiceCoding",
        "title": "Start Voice Coding",
        "category": "Voice Vibe Coding"
      },
      {
        "command": "voiceVibeCoding.stopVoiceCoding",
        "title": "Stop Voice Coding",
        "category": "Voice Vibe Coding"
      },
      {
        "command": "voiceVibeCoding.confirmCommand",
        "title": "Confirm Voice Command",
        "category": "Voice Vibe Coding"
      }
    ],
    "keybindings": [
      {
        "command": "voiceVibeCoding.startVoiceCoding",
        "key": "cmd+shift+v",
        "mac": "cmd+shift+v",
        "win": "ctrl+shift+v",
        "linux": "ctrl+shift+v",
        "when": "editorTextFocus"
      },
      {
        "command": "voiceVibeCoding.stopVoiceCoding",
        "key": "cmd+shift+v",
        "mac": "cmd+shift+v",
        "win": "ctrl+shift+v",
        "linux": "ctrl+shift+v",
        "when": "voiceVibeCoding.isListening"
      }
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "compile": "tsc",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src",
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
    "package": "npx @vscode/vsce package",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/mocha": "^10.0.0",
    "@types/vscode": "^1.80.0",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "mocha": "^10.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "@vscode/vsce": "^2.21.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/voice-vibe-team/voice-vibe-coding.git"
  },
  "bugs": {
    "url": "https://github.com/voice-vibe-team/voice-vibe-coding/issues"
  },
  "homepage": "https://github.com/voice-vibe-team/voice-vibe-coding#readme",
  "license": "MIT"
}
